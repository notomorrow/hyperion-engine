#importpath "../" "../library";

class TestScript {
    scene: Scene = null;
    entity: uint = ~0u;

    func on_added(scene: Scene, entity: uint) {
        self.scene = scene;
        self.entity = entity;
    }

    func on_removed() {
        self.scene = null;
        self.entity = ~0u;
    }

    func on_updated(delta: float) {
    }
}

export const controller = TestScript();